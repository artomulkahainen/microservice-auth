{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "importOrder": [
        "^@controllers/(.*)$",
        "^@dto/(.*)$",
        "^@mappers/(.*)$",
        "^@routes/(.*)$",
        "^@services/(.*)$",
        "^@utils/(.*)$",
        "^@validators/(.*)$",
        "^[./]"
